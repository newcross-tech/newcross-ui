{
  "calendar": {
    "font-family": { "value": "{brand.font-family.regular.value}" },
    "color": { "value": "{brand.color.primary.value}" },
    "font-size": { "value": "{typography.font-size.16.value}" },

    "header": {
      "font-weight": { "value": "{typography.font-weight.semi-bold.value}" },
      "weekdays": {
        "font-weight": { "value": "{typography.font-weight.regular.value}" }
      }
    },
    "days": {
      "font-size": { "value": "{typography.font-size.14.value}" },
      "disabled": {
        "color": { "value": "{color.neutral.grey.200.value}" }
      },
      "current-color": { "value": "{brand.color.primary.value}" },
      "booked": {
        "background-color": { "value": "{brand.color.secondary.300.value}" }
      },
      "unavailable": {
        "background-color": {
          "value": "{color.semantics.error.300.value}"
        }
      },
      "selected": {
        "background-color": { "value": "{brand.color.primary.value}" },
        "color": { "value": "{color.neutral.white.value}" }
      },
      "inactive": {
        "background-color": { "value": "{color.primary.warmth.value}" }
      },
      "date-range": {
        "background-color": { "value": "{color.primary.vigour.value}" }
      }
    }
  }
}
